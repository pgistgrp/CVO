<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>


    <bean name="/login" class="org.pgist.system.LoginAction">
        <property name="userDAO" ref="userDAO"/>
    </bean>
    
    
    <bean name="/logout" class="org.pgist.system.LogoutAction">
    </bean>
    
    
    <bean name="/register" class="org.pgist.system.RegisterAction">
        <property name="userDAO" ref="userDAO"/>
    </bean>
    
    
    <bean name="/search" class="org.pgist.search.SearchAction">
        <property name="searchHelper" ref="searchHelper"/>
        <property name="cvoDAO" ref="cvoDAO"/>
    </bean>
    
    
    <bean name="/cvolist" class="org.pgist.cvo.CVOListAction">
        <property name="userDAO" ref="userDAO"/>
        <property name="cvoDAO" ref="cvoDAO"/>
    </bean>
    
    
    <bean name="/cvoview" class="org.pgist.cvo.CVOViewAction">
        <property name="cvoDAO" ref="cvoDAO"/>
    </bean>
    
    
    <bean name="/concernManagement" class="org.pgist.cvo.ConcernManagementAction">
        <property name="cvoDAO" ref="cvoDAO"/>
    </bean>
    
    
    <!-- Workflow Management Actions -->
    
    
    <bean name="/situationList" class="org.pgist.workflow.SituationListAction">
        <property name="workflowDAO" ref="workflowDAO"/>
        <property name="engine" ref="engine"/>
    </bean>
    
    
    <bean name="/workflow" class="org.pgist.workflow.WorkflowAction">
        <property name="workflowDAO" ref="workflowDAO"/>
    </bean>
    
    
</beans>

